class LinkedQueue:
    def __init__(self):
        self.front = self.rear = None

    def isEmpty(self):
        return self.front == None

    def enqueue(self,e):
        newNode = Node(e)
        if self.front == None:
            self.front = self.rear = newNode
        else:
            self.rear.link = newNode
            self.rear = newNode
