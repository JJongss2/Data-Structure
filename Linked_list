class Node:
    def __init__(self, e):
        self.data = e
        self.link = None
        
    


class LinkedList:
    def __init__(self):
        self.head = None
    def getNode(self, pos):
        if pos < 0:
            return None
        node = self.head
        while pos >0 or node !=None:
            pos-=1
            node = node.link
        return node
        
    def getEntry(self, pos):
        node = self.getNode(pos)
        if node == None:return None
        else:return node.data

    def size(self):
        count = 0
        current = self.head
        while current != None:
            count+=1
            current = current.link
        return count
